{
  "name": "medi-mind",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:netlify": "netlify dev",
    "dev:with-stagewise": "concurrently \"vite --port 5174\" \"npx stagewise@latest --app-port 5174 --port 3001\"",
    "dev:netlify-stagewise": "concurrently \"netlify dev\" \"npx stagewise@latest --app-port 8888 --port 3001\"",
    "stagewise": "npx stagewise@latest",
    "stagewise:bridge": "npx stagewise@latest -b",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare": "husky",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:medical": "vitest run --reporter=verbose src/__tests__/medical",
    "test:search": "vitest run --reporter=verbose src/__tests__/components/MediSearch",
    "test:integration": "vitest run --reporter=verbose src/__tests__/integration",
    "test:e2e": "playwright test",
    "test:performance": "vitest run --reporter=verbose src/__tests__/performance",
    "test:accessibility": "vitest run --reporter=verbose src/__tests__/accessibility",
    "cleanup:console": "node scripts/remove-console-logs.js",
    "cleanup:build": "rm -rf .netlify/functions-serve dist node_modules/.vite",
    "cleanup:ocr": "node scripts/migrate-ocr-imports.js",
    "cleanup:advanced": "npm run cleanup:console && npm run cleanup:ocr",
    "cleanup": "npm run cleanup:console && npm run cleanup:build",
    "deploy:prod": "npm run build && netlify deploy --prod",
    "migrate:prod": "node scripts/migrate-production.js",
    "migrate:prod:dry": "node scripts/migrate-production.js --dry-run",
    "rollback:prod": "node scripts/rollback-production.js",
    "rollback:prod:dry": "node scripts/rollback-production.js --dry-run",
    "health:check": "curl -f https://medimindexpert.netlify.app/api/system/health || echo 'Health check failed'"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.0.1",
    "@netlify/functions": "^4.0.0",
    "@radix-ui/react-select": "^2.2.5",
    "@supabase/supabase-js": "^2.53.0",
    "@tailwindcss/typography": "^0.5.16",
    "axios": "^1.8.4",
    "base64-arraybuffer": "^1.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dompurify": "^3.2.6",
    "express": "^5.1.0",
    "form-data": "^4.0.3",
    "framer-motion": "^11.18.2",
    "html2canvas": "^1.4.1",
    "i18next": "^25.2.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.344.0",
    "marked": "^12.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^5.5.1",
    "@elevenlabs/elevenlabs-js": "^2.0.0",
    "pdfjs-dist": "^5.3.93",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.56.4",
    "react-hot-toast": "^2.5.2",
    "react-hotkeys-hook": "^4.6.1",
    "react-i18next": "^15.5.2",
    "react-markdown": "^9.1.0",
    "react-router-dom": "^6.22.2",
    "react-syntax-highlighter": "^15.6.1",
    "recharts": "^2.12.2",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tesseract.js": "^6.0.1",
    "unist-util-visit": "^5.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.31",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/jspdf": "^2.0.0",
    "@types/marked": "^5.0.2",
    "@types/mdast": "^4.0.4",
    "@types/node-fetch": "^2.6.12",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/unist": "^3.0.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^25.0.1",
    "msw": "^2.6.5",
    "netlify-cli": "^21.5.0",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.1",
    "terser": "^5.27.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^2.1.8"
  }
}
